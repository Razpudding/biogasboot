<body id="customer-dashboard" class="background">
  <header>
    <h1>De Biogasboot</h1>
    <p>Het effect sinds <time>06-06-2017</time></p>
  </header>
  <section class="card-dashboard">
    <article id="food-waste" class="card">
      <h1></h1>
      <img src="/images/dashboard/recycle-bin.svg" alt="Bin icon">
      <header>
        <h1>Recycling van Afval</h1>
        <div class="info">
          <h3 class="effect">80kg sinds ...</h3>
          <p class="fact">Per dag gaat er 10kg afval naar de biogasboot</p>
        </div>
      </header>
    </article>
    <article id="gas-produced" class="card">
      <img src="/images/dashboard/biofuel.svg" alt="Biogas icon">
      <header>
        <h1>Produceert Biogas</h1>
        <div class="info">
          <h3 class="effect">80L sinds ...</h3>
          <p class="fact">Elke dag worden er gemiddeld 10 gerechten puur op biogas klaargemaakt</p>
        </div>
      </header>
    </article>
    <article id="carbon-reduced" class="card">
      <img src="/images/dashboard/co2-inside-cloud.svg" alt="Cloud icon">
      <header>
        <h1>Reduceert CO2 uitstoot</h1>
        <div class="info">
          <h3 class="effect">80kg/h/l sinds ...</h3>
          <p class="fact">Per dag gaat er 10kg afval naar de biogasboot</p>
        </div>
      </header>
    </article>
