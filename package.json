{
  "name": "biogasboot",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "start-update": "nodemon ./bin/www",
    "expose": "ngrok http 3000",
    "build-js": "mkdir -p public/js && browserify ./src/js/index.js -o ./public/js/index.js -t [ babelify --presets es2015 ]",
    "build-css": "mkdir -p public/css && node-sass --include-path scss src/scss/main.scss ./public/css/main.css",
    "build-webp": "node ./npm-scripts/webp.js",
    "build": "mkdir -p public/js && mkdir -p public/css && npm run build-js && npm run build-css",
    "watch-js": "mkdir -p public/js && watchify ./src/js/index.js -o ./public/js/index.js -t [ babelify --presets es2015 ]",
    "watch-css": "mkdir -p public/css && nodemon -e scss -x \"npm run build-css\"",
    "watch": "mkdir -p public/js && mkdir -p public/css && concurrently --kill-others \"npm run watch-js\" \"npm run watch-css\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sjoerdbeentjes/biogasboot.git"
  },
  "keywords": [],
  "author": "",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/sjoerdbeentjes/biogasboot/issues"
  },
  "homepage": "https://github.com/sjoerdbeentjes/biogasboot#readme",
  "xo": {
    "space": true,
    "env": [
      "node",
      "browser"
    ],
    "rules": {
      "capitalized-comments": 0,
      "operator-linebreak": 0,
      "camelcase": 0,
      "no-implicit-coercion": 0,
      "no-unused-vars": 0,
      "new-cap": 0,
      "no-multi-assign": 0,
      "curly": 0,
      "no-else-return": 0,
      "array-callback-return": 0,
      "dot-notation": 0,
      "no-unused-expressions": [
        "error",
        {
          "allowTernary": true
        }
      ],
      "unicorn/no-new-buffer": "off",
      "unicorn/filename-case": "off",
      "unicorn/custom-error-definition": "off",
      "unicorn/no-array-instanceof": "off",
      "unicorn/catch-error-name": "off",
      "unicorn/no-process-exit": "off",
      "unicorn/throw-new-error": "off",
      "unicorn/number-literal-case": "off",
      "unicorn/prefer-starts-ends-with": "off",
      "unicorn/prefer-type-error": "off",
      "unicorn/explicit-length-check": "off",
      "unicorn/no-abusive-eslint-disable": "off"
    }
  },
  "dependencies": {
    "bcryptjs": "*",
    "body-parser": "~1.17.1",
    "concurrently": "^3.4.0",
    "connect-flash": "*",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.3",
    "csv-parse": "^1.2.0",
    "d3": "^4.9.1",
    "debug": "~2.6.3",
    "dotenv": "^4.0.0",
    "ejs": "~2.5.6",
    "express": "~4.15.2",
    "express-messages": "*",
    "express-session": "*",
    "express-validator": "*",
    "feed-read": "0.0.1",
    "jsftp": "^2.0.0",
    "line-by-line": "^0.1.5",
    "moment": "^2.18.1",
    "mongodb": "*",
    "mongoose": "^4.10.3",
    "morgan": "~1.8.1",
    "node-sass-middleware": "0.9.8",
    "passport": "*",
    "passport-http": "*",
    "passport-local": "*",
    "request": "^2.81.0",
    "socket.io": "^2.0.2",
    "socket.io-client": "^2.0.2",
    "web-push": "^3.2.2"
  },
  "devDependencies": {
    "xo": "^0.18.2",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "imagemin": "^5.3.1",
    "imagemin-webp": "^4.0.0",
    "ngrok": "^2.2.9",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "watchify": "^3.9.0"
  }
}
